import "liblemon/io.lime"
import "liblemon/string.lime"
import "liblemon/mem.lime"

Token: struct {
    type: u32;
    text: String;
    indent: u32;
    line: u32;
    col: u32;
}

printtoken: fn(tok: Token*) = {
    putstr(tok.text&);
}

main: fn() -> i32 = {
    let data = malloc(10);
    memset(data, 65, 10);
    let lol = String { length: 10, data: data };
    putstr(lol&);
    let tok = Token { type: 0, text: lol, indent: 1, line: 69, col: 420 };
    printtoken(tok&);
    0
}
